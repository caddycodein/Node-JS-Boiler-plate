{
  "name": "node-js-boiler-plate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "types": "./types/global.d.ts",
  "scripts": {
    "start": "nodemon index.js",
    "build": "tsc",
    "start:local": "npm run lint && NODE_ENV=local nodemon index.js",
    "start:test": "npm run lint && NODE_ENV=test node build/server.js",
    "start:staging": "npm run lint && NODE_ENV=staging node build/server.js",
    "start:production": "npm run lint && NODE_ENV=production node build/server.js",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "test:custom": "npm test -- tests/unit-tests/lib/crypto.spec.ts",
    "precommit": "npm run lint-fix && npm run pretty",
    "prepush": "npm run lint ",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "typeorm": "node --require ts-node/register node_modules/typeorm/cli.js",
    "migration-dev:generate": "npm run build && npm run typeorm migration:generate ./src/migrations/dev-migrations/postrefactoring -- -d ./devormconfig.ts",
    "migration-qa:generate": "npm run build && npm run typeorm migration:generate ./src/migrations/qa-migrations/postrefactoring -- -d ./qaormconfig.ts",
    "migration-uat:generate": "npm run build && npm run typeorm migration:generate ./src/migrations/uat-migrations/postrefactoring -- -d ./uatormconfig.ts",
    "migration-eu-prod:generate": "npm run build && npm run typeorm migration:generate ./src/migrations/eu-prod-migrations/postrefactoring -- -d ./euprodormconfig.ts",
    "migration-us-prod:generate": "npm run build && npm run typeorm migration:generate ./src/migrations/us-prod-migrations/postrefactoring -- -d ./usprodormconfig.ts",
    "migration-dev:up": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d devormconfig.ts",
    "migration-qa:up": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d qaormconfig.ts",
    "migration-uat:up": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d uatormconfig.ts",
    "migration-eu-prod:up": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d euprodormconfig.ts",
    "migration-us-prod:up": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d usprodormconfig.ts",
    "migration:down": "ts-node --transpile-only ./node-modules/typeorm/cli.js migration:revert"
  },
  "author": "shashank-6storage",
  "dependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.8.3",
    "@types/validator": "^13.7.7",
    "@types/winston": "^2.4.4",
    "axios": "^1.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "helmet": "^5.1.1",
    "http-status-codes": "^2.2.0",
    "mssql": "^7.3.5",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.25.2",
    "sequelize-cli-typescript": "^3.2.0-c",
    "sequelize-mig": "^3.1.3",
    "serverless-http": "^3.0.3",
    "tedious": "^15.1.0",
    "typeorm": "^0.3.10",
    "typescript": "^4.8.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.8.3",
    "@types/supertest": "^2.0.12",
    "@types/tedious": "^4.0.9",
    "@types/validator": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.3.0",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.1"
  }
}
